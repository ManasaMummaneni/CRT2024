Switch condition
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int a=1;
    switch(a)
    {
    case 1:
    printf("Java");
    break;
    case 2:
    printf("Python");
    break;
    case 3:
    printf("C");
    break;
    default:
    printf("Invalid");
    }
    return 0;
}
